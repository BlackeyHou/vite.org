    "@fortawesome/vue-fontawesome": "^0.1.10",
    "@nuxtjs/axios": "^5.1.1",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/proxy": "^2.0.0",
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "@nuxtjs/sitemap": "^2.4.0",
    "@nuxtjs/webpackmonitor": "^0.1.0",
    "animate-canvas-line": "^1.1.0",
